#include "mux2.loong"

hmodule mux4 {
public:
  mux4(input  d0[3],
       input  d1[3],
       input  d2[3],
       input  d3[3],
       input  s[1],
       output y[3]) {
    wire low[3], high[3];
    mux2 lowmux(d0, d1, s[0], low);
    mux2 highmux(d2, d3, s[0], high);
    mux2 finalmux(low, high, s[1], y);
  }
};
